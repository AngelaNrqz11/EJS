<%- include("partials/header"); -%>

<h1>Home</h1>
<p><%= content %></p>
<form action="/compose" method="GET">
    <button>Compose</button>
</form>

<% contentArray.forEach(element => { %>
<h1><%= element.title %></h1>
<p>
    <%= element.body.substring(0, 100) %> ...
    <a href="/posts/<%=element.title%>">Read More</a>
</p>
<% }); %>

<!-- footer -->
<%- include("partials/footer"); -%>
